{
  "name": "api-gateway",
  "version": "2.5.3",
  "description": "L'api c'est la vie ! Tout converge vers le gateway.",
  "scripts": {
    "database:rethinkdb:backup": "clear && node tools/rethink_backup.js",
    "database:mysql:create:answers": "clear && node tools/mysql_create_answers.js",
    "database:mysql:insert:answers": "clear && node tools/mysql_insert_answers.js",
    "database:mysql:create:users": "clear && node tools/mysql_create_users.js",
    "database:mysql:insert:users": "clear && node tools/mysql_insert_users.js",
    "test:coverage": "clear && istanbul cover ./node_modules/.bin/_mocha -- --timeout 15000 -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "auto:generate": "clear && ./bin/abibao generate",
    "start": "clear && DEBUG=abibao*,servicebus* ABIBAO_API_GATEWAY_NEWRELIC_APPNAME=api.deve.abibao.com node . | bunyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/abibao/api-gateway.git"
  },
  "keywords": [
    "api",
    "gateway",
    "abibao"
  ],
  "author": "team abibao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/abibao/api-gateway/issues"
  },
  "homepage": "https://gitlab.com/abibao/api-gateway#README",
  "engines": {
    "node": "=5.5"
  },
  "dependencies": {
    "async": "2.0.0-rc.5",
    "base64-url": "1.2.2",
    "bluebird": "3.4.0",
    "boom": "3.2.1",
    "bunyan": "1.8.1",
    "bunyan-logstash-tcp": "0.3.4",
    "chance": "1.0.3",
    "crumb": "6.0.1",
    "crypto": "0.0.3",
    "cryptr": "2.0.0",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "fs-extra": "0.30.0",
    "good": "6.6.3",
    "good-bunyan": "0.1.2",
    "hapi": "13.4.1",
    "hapi-auth-basic": "4.2.0",
    "hapi-auth-jwt2": "7.0.1",
    "hoek": "4.0.0",
    "inert": "4.0.0",
    "iron": "4.0.1",
    "joi": "8.4.0",
    "jsonwebtoken": "7.0.0",
    "knex": "0.11.5",
    "lodash": "4.13.1",
    "markdown": "0.5.0",
    "md5": "2.1.0",
    "mongoose": "4.4.19",
    "mysql": "2.10.2",
    "nconf": "0.8.4",
    "newrelic": "1.28.0",
    "node-dir": "0.1.12",
    "node-uuid": "1.4.7",
    "recursive-readdir": "2.0.0",
    "request-promise": "3.0.0",
    "resolve-app-pkginfo": "1.0.0",
    "sendgrid": "2.0.0",
    "servicebus": "2.0.2",
    "standard": "7.1.1",
    "thinky": "2.3.2",
    "vision": "4.1.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cli-color": "1.1.0",
    "codacy-coverage": "1.1.3",
    "commander": "2.9.0",
    "faker": "3.1.0",
    "js-object-pretty-print": "0.2.0",
    "mocha": "2.5.3",
    "mustache": "2.2.1",
    "rethinkdb": "2.3.1",
    "standard": "7.1.1"
  }
}
