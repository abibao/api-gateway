{
  "name": "api-gateway",
  "version": "2.6.6",
  "description": "L'api c'est la vie ! Tout converge vers le gateway.",
  "scripts": {
    "auto:generate": "./npm-scripts/auto-generate.sh",
    "database:rethinkdb:import:rece": "clear && node tools/rethink_import_rece.js",
    "database:rethinkdb:backup": "clear && node tools/rethink_backup.js",
    "database:mysql:create:answers": "clear && node tools/mysql_create_answers.js",
    "database:mysql:insert:answers": "clear && node tools/mysql_insert_answers.js",
    "database:mysql:create:users": "clear && node tools/mysql_create_users.js",
    "database:mysql:insert:users": "clear && node tools/mysql_insert_users.js",
    "database:mysql:import:t_users": "clear && node tools/mysql_import_t_users.js",
    "crons": "./npm-scripts/crons.sh",
    "security:wizard": "clear && ./node_modules/.bin/snyk wizard",
    "security:protect": "clear && ./node_modules/.bin/snyk protect",
    "test:security": "clear && ./node_modules/.bin/snyk test",
    "test:coverage": "clear && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --timeout 15000 -R spec",
    "test:all": "./npm-scripts/test-all.sh",
    "start": "clear && DEBUG=abibao*,servicebus* ABIBAO_API_GATEWAY_NEWRELIC_APPNAME=api.deve.abibao.com node . | bunyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/abibao/api-gateway.git"
  },
  "keywords": [
    "api",
    "gateway",
    "abibao"
  ],
  "author": "team abibao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/abibao/api-gateway/issues"
  },
  "homepage": "https://gitlab.com/abibao/api-gateway#README",
  "engines": {
    "node": "=6.2.1"
  },
  "dependencies": {
    "async": "2.0.0-rc.6",
    "base64-url": "1.2.2",
    "bluebird": "3.4.1",
    "boom": "3.2.2",
    "bson": "0.4.23",
    "bunyan": "1.8.1",
    "bunyan-logstash-tcp": "0.3.4",
    "crumb": "6.0.1",
    "cryptr": "2.0.0",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "fs-extra": "0.30.0",
    "good-bunyan": "0.1.2",
    "hapi": "13.4.1",
    "hapi-auth-basic": "4.2.0",
    "hapi-auth-jwt2": "7.0.1",
    "hoek": "4.0.1",
    "inert": "4.0.0",
    "iron": "4.0.1",
    "joi": "8.4.2",
    "jsonwebtoken": "7.0.1",
    "knex": "0.11.7",
    "lodash": "4.13.1",
    "md5": "2.1.0",
    "mysql": "2.11.1",
    "nconf": "0.8.4",
    "newrelic": "1.28.1",
    "node-uuid": "1.4.7",
    "recursive-readdir": "2.0.0",
    "request-promise": "3.0.0",
    "sendgrid": "3.0.5",
    "servicebus": "2.0.2",
    "thinky": "2.3.2",
    "vision": "4.1.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cli-color": "1.1.0",
    "codacy-coverage": "1.1.3",
    "commander": "2.9.0",
    "faker": "3.1.0",
    "istanbul": "0.4.4",
    "js-object-pretty-print": "0.2.0",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.2.0",
    "mustache": "2.2.1",
    "request-promise": "3.0.0",
    "rethinkdb": "2.3.2",
    "snyk": "1.15.0",
    "standard": "7.1.2"
  }
}
