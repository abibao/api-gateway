{
  "name": "api-gateway",
  "version": "3.0.0",
  "description": "L'api c'est la vie ! Tout converge vers le gateway.",
  "scripts": {
    "check:updates": "./node_modules/.bin/ncu",
    "auto:generate": "./npm-scripts/auto-generate.sh",
    "crons": "./npm-scripts/crons.sh",
    "backups:prod": "clear && node ./tools/backups.js --rethink=prodmvp",
    "restore:rece-from-prod": "clear && node ./tools/restore.js --from=prodmvp --to=recemvp",
    "security:wizard": "clear && ./node_modules/.bin/snyk wizard",
    "security:protect": "clear && ./node_modules/.bin/snyk protect",
    "test:security": "clear && ./node_modules/.bin/snyk test",
    "test:coverage": "clear && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --timeout 15000 -R spec",
    "test:all": "./npm-scripts/test-all.sh",
    "start": "clear && DEBUG=abibao*,servicebus* ABIBAO_API_GATEWAY_NEWRELIC_APPNAME=api.deve.abibao.com node . | bunyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/abibao/api-gateway.git"
  },
  "keywords": [
    "api",
    "gateway",
    "abibao"
  ],
  "author": "team abibao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/abibao/api-gateway/issues"
  },
  "homepage": "https://gitlab.com/abibao/api-gateway#README",
  "engines": {
    "node": "=5.12"
  },
  "dependencies": {
    "async": "2.0.0-rc.6",
    "base64-url": "1.2.2",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "boom": "3.2.2",
    "bson": "0.5.2",
    "bunyan": "1.8.1",
    "bunyan-logstash-tcp": "0.3.4",
    "compression": "1.6.2",
    "cors": "2.7.1",
    "crumb": "6.0.2",
    "cryptr": "2.0.0",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "faker": "3.1.0",
    "feathers": "2.0.1",
    "feathers-authentication": "0.7.9",
    "feathers-configuration": "0.3.0",
    "feathers-errors": "2.3.0",
    "feathers-hooks": "1.5.4",
    "feathers-rest": "1.4.2",
    "feathers-rethinkdb": "0.2.1",
    "feathers-socketio": "1.4.1",
    "fs-extra": "0.30.0",
    "good-bunyan": "0.1.2",
    "hapi": "13.5.0",
    "hapi-auth-basic": "4.2.0",
    "hapi-auth-jwt2": "7.0.1",
    "hoek": "4.0.1",
    "inert": "4.0.1",
    "iron": "4.0.1",
    "joi": "9.0.0",
    "jsonwebtoken": "7.0.1",
    "knex": "0.11.7",
    "lodash": "4.13.1",
    "md5": "2.1.0",
    "mysql": "2.11.1",
    "nconf": "0.8.4",
    "newrelic": "1.28.2",
    "node-uuid": "1.4.7",
    "recursive-readdir": "2.0.0",
    "request-promise": "3.0.0",
    "rethinkdbdash": "2.3.16",
    "sendgrid": "3.0.7",
    "serve-favicon": "2.3.0",
    "servicebus": "2.0.2",
    "thinky": "2.3.3",
    "vision": "4.1.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "cli-color": "1.1.0",
    "codacy-coverage": "1.1.3",
    "commander": "2.9.0",
    "istanbul": "0.4.4",
    "js-object-pretty-print": "0.2.0",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.2.0",
    "mustache": "2.2.1",
    "npm-check-updates": "2.7.0",
    "request-promise": "3.0.0",
    "rethinkdb": "2.3.2",
    "snyk": "1.17.0",
    "standard": "7.1.2"
  }
}
