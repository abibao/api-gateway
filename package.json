{
  "name": "api-gateway",
  "version": "3.1.0",
  "description": "L'api c'est la vie ! Tout converge vers le gateway.",
  "scripts": {
    "setup:deve": "npm-scripts/setup-deve.sh",
    "setup:clean": "npm-scripts/setup-clean.sh",
    "batch:import": "./src/batchs/import.sh",
    "test": "npm-scripts/test.sh",
    "test:coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --opts .mocharc",
    "test:standard": "node_modules/.bin/standard --verbose --fix | snazzy",
    "test:security": "node_modules/.bin/snyk test",
    "start": "DEBUG=abibao:mysql,abibao:application ABIBAO_API_GATEWAY_NEWRELIC_APPNAME=api.deve.abibao.com node . | bunyan",
    "start:deve": "DEBUG=abibao*,servicebus* ABIBAO_API_GATEWAY_NEWRELIC_APPNAME=api.deve.abibao.com node src-v3/ | bunyan"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "before",
      "after",
      "describe",
      "it"
    ],
    "ignore": [
      "src/www/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/abibao/api-gateway.git"
  },
  "keywords": [
    "api",
    "gateway",
    "abibao"
  ],
  "author": "team abibao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/abibao/api-gateway/issues"
  },
  "homepage": "https://gitlab.com/abibao/api-gateway#README",
  "engines": {
    "node": "~6"
  },
  "dependencies": {
    "async": "2.1.4",
    "base64-url": "1.3.3",
    "bluebird": "3.4.7",
    "boom": "4.2.0",
    "bson": "1.0.3",
    "bunyan": "1.8.5",
    "bunyan-logstash-tcp": "0.3.5",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "cron": "1.2.1",
    "crumb": "6.0.3",
    "cryptr": "2.0.0",
    "debug": "2.6.0",
    "download": "5.0.2",
    "eraro": "0.4.1",
    "faker": "3.1.0",
    "fast-csv": "2.3.0",
    "fs-extra": "1.0.0",
    "glob": "7.1.1",
    "hapi": "16.1.0",
    "hapi-auth-jwt2": "7.2.4",
    "hoek": "4.1.0",
    "inert": "4.1.0",
    "iron": "4.0.4",
    "joi": "10.1.0",
    "jsonwebtoken": "7.2.1",
    "knex": "0.12.6",
    "line-by-line": "0.1.5",
    "lodash": "4.17.4",
    "md5": "2.2.1",
    "mysql": "2.12.0",
    "nconf": "0.8.4",
    "nes": "6.4.0",
    "newrelic": "1.36.0",
    "node-uuid": "1.4.7",
    "progress": "1.1.8",
    "progress-promise": "0.0.5",
    "prompt": "1.0.0",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "rethinkdbdash": "2.3.27",
    "sendgrid": "4.7.1",
    "sequelize": "3.29.0",
    "servicebus": "2.0.7",
    "servicebus-retry": "0.0.11",
    "thinky": "2.3.8",
    "vision": "4.1.1",
    "wpapi": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "cli-color": "1.1.0",
    "codacy-coverage": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.2.0",
    "mocha-standard": "1.0.0",
    "request-promise": "4.1.1",
    "rethinkdb": "2.3.3",
    "sinon": "1.17.7",
    "snazzy": "5.0.0",
    "snyk": "1.23.2",
    "standard": "8.6.0"
  }
}
